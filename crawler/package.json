{
  "name": "reefexplorer-crawler",
  "version": "1.0.1",
  "author": "Žiga Franko Gorišek",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.production.json",
    "start": "node --max-old-space-size=16384 ./dist/crawler.js",
    "prod": "tsc -p tsconfig.production.json && node --max-old-space-size=16384 ./dist/crawler.js",
    "profile": "tsc && node --prof --max-old-space-size=16384 ./dist/crawler.js",
    "dev": "NODE_OPTIONS=--max-old-space-size=16384 nodemon ./src/crawler.ts",
    "start-backtracking": "node --max-old-space-size=16384 ./dist/backtracking.js",
    "prod-backtracking": "tsc -p tsconfig.production.json && node --max-old-space-size=16384 ./dist/backtracking.js",
    "profile-backtracking": "tsc && node --prof --max-old-space-size=16384 ./dist/backtracking.js",
    "dev-backtracking": "NODE_OPTIONS=--max-old-space-size=16384 nodemon ./src/backtracking.ts",
    "dev-pool": "NODE_OPTIONS=--max-old-space-size=16384 nodemon ./src/pool.ts",
    "start-pool": "node --max-old-space-size=16384 ./dist/pool.js",
    "test": "echo skip",
    "lint": "eslint --fix --ext .ts ./src/"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/pg": "^8.6.1",
    "@types/pg-format": "^1.0.2",
    "@types/pino": "^7.0.5",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@reef-defi/evm-provider": "^1.0.6",
    "@sentry/integrations": "^6.16.1",
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.3.0",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4",
    "pino": "^7.5.1",
    "pino-pretty": "^7.3.0"
  }
}
